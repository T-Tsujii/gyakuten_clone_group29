<h1>新規投稿</h1>
<%= form_with model: question, local: true do |form| %>
  <div>
    <%= form.label :title, "質問" %>
    <%= form.text_field :title, size: 8 %>
  </div>
  <div>
    <%= form.label :detail, "詳細" %>
    <%= form.text_field :detail, size: 8 %>
  </div>
  <div>
    <%= form.submit "送信" %>
  </div>
<% end %>
